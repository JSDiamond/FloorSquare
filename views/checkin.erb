<table id="checkins">
	<thead>
		<tr>
			<th>First Name</th>
			<th>Last Name</th>
			<th>Project(s)</th>
			<th>Check In Time</th>
		</tr>
	</thead>
	<tbody>
		<% @swipes.each do |swipe| %>
			<tr>
				<td><%= swipe.user.first %></td>
				<td><%= swipe.user.last %></td>
				<td>-</td>
				<td><%= swipe.created_at %></td>
			</tr>
		<% end %>
	</tbody>
</table>